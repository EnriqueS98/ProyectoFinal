<% include _header%>
<div class="main-container">
<div class="main-forms" style="display: block;">
    <div class="signup-form">
        <form class="sign-back" action="/singin" method="post">
        <div class="fpchapterstitleregister">
            <span>Sing In</span>
        </div>
            <div class="form-group">
                <input name="username" type="text" class="form-control"
                placeholder="Nombre Del Usuario" required autofocus/>
            </div>
            <div class="form-group">
                <input name="password" type="password" class="form-control"
                placeholder="Contrasena" required autofocus/>
            </div>
            <input type="submit" value="Entrar"  class="btn btn-dark btn-block"/>
        </form>
    </div>
</div>
<!-- End Main Forms -->
</div>

<% include _footer%>