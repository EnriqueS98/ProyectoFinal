<% include _header %>

<h1> Mostrar Alumnos</h1>

<% evaluacion.forEach((alumno)=>{ %>
<div class="panel panel-default">
    
    
        <div class="panel-heading">
            <a>
            <label  class="col-form-label">Nombre:</label>
                <%= alumno.nombre %>
            </a>
        </div>
        <div class="row"class="panel-body">
                <label  class="col-md-1 col-form-label">Apellido:</label>
                <label  class="col-form-label"><%= alumno.apellido %></label>
        </div>
        <div class="row">
            <label  class="col-md-1 col-form-label">Sexo:</label>
            <label  class="col-form-label"><%= alumno.sexo %></label>
        </div>
        <div class="row">
            <label  class="col-md-1 col-form-label">Matricula:</label>
            <label  class="col-form-label"><%= alumno.matricula %></label>
        </div>
        <div class="row">
            <label  class="col-md-1 col-form-label">Calificacion:</label>
            <label  class="col-form-label"><%= alumno.calificacion %></label>
        </div>
        
</div>
    
   

    <% }) %>
   
    
 

    <% include _footer %>